/*-----------------------------------------------------------------------------
 * Include 
 *-----------------------------------------------------------------------------*/
#include <stdio.h>
#include "system.h"
#include "string.h"

/*-----------------------------------------------------------------------------
 *  Variable
 *-----------------------------------------------------------------------------*/
unsigned short * ram = (unsigned short *)(SDRAM_BASE+0x10000); //SDRAM地址


int main(void)
{
    int i;
    
    memset(ram,0,100);


    for(i=0;i<100;i++){
        *(ram++) = i;
    }

    for(i=0;i<100;i++){
        printf("%d\n",*(--ram));
    }
       
    return 0;
}

