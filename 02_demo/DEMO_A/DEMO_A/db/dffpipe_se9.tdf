--synthesis_resources = reg 10 
OPTIONS ALTERA_INTERNAL_OPTION = "AUTO_SHIFT_REGISTER_RECOGNITION=OFF";

SUBDESIGN dffpipe_se9
( 
	clock	:	input;
	clrn	:	input;
	d[9..0]	:	input;
	q[9..0]	:	output;
) 
VARIABLE 
	dffe16a[9..0] : dffe;
	ena	: NODE;
	prn	: NODE;
	sclr	: NODE;

BEGIN 
	dffe16a[].clk = clock;
	dffe16a[].clrn = clrn;
	dffe16a[].d = (d[] & (! sclr));
	dffe16a[].ena = ena;
	dffe16a[].prn = prn;
	ena = VCC;
	prn = VCC;
	q[] = dffe16a[].q;
	sclr = GND;
END;
--VALID FILE
