--synthesis_resources = lut 4 
SUBDESIGN decode_msa
( 
	data[1..0]	:	input;
	enable	:	input;
	eq[3..0]	:	output;
) 
VARIABLE 
	data_wire[1..0]	: WIRE;
	enable_wire	: WIRE;
	eq_node[3..0]	: WIRE;
	eq_wire[3..0]	: WIRE;
	w_anode2556w[2..0]	: WIRE;
	w_anode2569w[2..0]	: WIRE;
	w_anode2577w[2..0]	: WIRE;
	w_anode2585w[2..0]	: WIRE;

BEGIN 
	data_wire[] = data[];
	enable_wire = enable;
	eq[] = eq_node[];
	eq_node[3..0] = eq_wire[3..0];
	eq_wire[] = ( w_anode2585w[2..2], w_anode2577w[2..2], w_anode2569w[2..2], w_anode2556w[2..2]);
	w_anode2556w[] = ( (w_anode2556w[1..1] & (! data_wire[1..1])), (w_anode2556w[0..0] & (! data_wire[0..0])), enable_wire);
	w_anode2569w[] = ( (w_anode2569w[1..1] & (! data_wire[1..1])), (w_anode2569w[0..0] & data_wire[0..0]), enable_wire);
	w_anode2577w[] = ( (w_anode2577w[1..1] & data_wire[1..1]), (w_anode2577w[0..0] & (! data_wire[0..0])), enable_wire);
	w_anode2585w[] = ( (w_anode2585w[1..1] & data_wire[1..1]), (w_anode2585w[0..0] & data_wire[0..0]), enable_wire);
END;
